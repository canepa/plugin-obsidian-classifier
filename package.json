{
  "name": "auto-tagger",
  "version": "2.0.2",
  "description": "Multi-collection semantic tag suggestions with embedding-based classifiers",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "deploy": "npm run build && powershell -File deploy.ps1",
    "watch": "node esbuild.config.mjs && powershell -File deploy.ps1"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "tagger",
    "classifier",
    "collections",
    "semantic",
    "embeddings"
  ],
  "author": "Alessandro Canepa",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "typescript": "4.7.4"
  }
}
