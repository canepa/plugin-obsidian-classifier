{
  "name": "auto-tagger",
  "version": "2.0.7",
  "description": "Multi-collection semantic tag suggestions with embedding-based classifiers",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "npm run lint && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:ci": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "deploy": "npm run build && powershell -File deploy.ps1",
    "watch": "node esbuild.config.mjs && powershell -File deploy.ps1"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "tagger",
    "classifier",
    "collections",
    "semantic",
    "embeddings"
  ],
  "author": "Alessandro Canepa",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.39.2",
    "obsidian": "latest",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.51.0"
  }
}
